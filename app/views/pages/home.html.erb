<h1>Isabel Zhang's Homework 3 </h1>

<body background="https://s-media-cache-ak0.pinimg.com/564x/c4/81/4b/c4814ba9cf0938e30ba1ef761bf4fcdb.jpg">
</body>

<font size="3" color="white" face="verdana">
<table>
  <tr>
    <th>User</th>
  </tr>
  <% @user.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.age %></td>
      <td><%= user.email %></td>

    </tr>
  <% end %>
</table>
</font>

<font size="3" color="white" face="verdana">
<table>
  <tr>
    <th>Cats</th>
  </tr>
  <% @cat.each do |cat| %>
    <tr>
      <td><%= cat.name %></td>
    </tr>
  <% end %>
</table>
</font>

<font size="3" color="white" face="verdana">
<table>
  <tr>
    <th>Todo</th>
  </tr>
  <% @todo.each do |todo| %>
    <tr>
      <td><%= todo.name %></td>
      <td><%= todo.description %></td>

    </tr>
  <% end %>
</table>
</font>